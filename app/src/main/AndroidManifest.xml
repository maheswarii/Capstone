<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.yps.layani">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Layani"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".admin.ui.detail.DetailDoneActivity"/>
        <activity android:name=".admin.ui.detail.DetailInfoActivity" />
        <activity android:name=".admin.ui.detail.DetailComplaintActivity" />
        <activity
            android:name=".admin.start.HomeActivity"
            android:label="@string/title_activity_home" />
        <activity android:name=".admin.start.RegisterActivity" />
        <activity android:name=".admin.start.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>